```xml
<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="1.2" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ChatGPT UML Generator</XMI.exporter>
      <XMI.exporterVersion>1.0</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model name="untitledModel" isSpecification="false">
      <UML:Namespace.ownedElement>

        <!-- Klasse TemplatedDocument -->
        <UML:Class name="TemplatedDocument" visibility="public" isAbstract="false"/>

        <!-- Stereotyp implementation -->
        <UML:Stereotype name="implementation">
          <UML:Stereotype.baseClass>Class</UML:Stereotype.baseClass>
        </UML:Stereotype>

        <!-- Stereotyp atdocument -->
        <UML:Stereotype name="atdocument">
          <UML:Stereotype.baseClass>Class</UML:Stereotype.baseClass>
        </UML:Stereotype>

        <!-- Anwendung der Stereotypen auf die Klasse -->
        <UML:ModelElement.stereotype>
          <UML:Stereotype name="implementation"/>
          <UML:Stereotype name="atdocument"/>
        </UML:ModelElement.stereotype>

      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>
