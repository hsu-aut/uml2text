```xml
<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ChatGPT UML Generator</XMI.exporter>
      <XMI.exporterVersion>1.0</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="2.0"/>
  </XMI.header>
  <XMI.content>
    <UML:Model name="grUML-1.0.1" isSpecification="false">
      <UML:Namespace.ownedElement>

        <!-- Stereotypen -->
        <UML:Stereotype name="profile" visibility="public"/>
        <UML:Stereotype name="graphclass" visibility="public">
          <UML:Generalization>
            <UML:Generalization.general>
              <UML:Class href="http://www.omg.org/spec/UML/20090901#Class"/>
            </UML:Generalization.general>
          </UML:Generalization>
        </UML:Stereotype>

        <UML:Stereotype name="record" visibility="public">
          <UML:Generalization>
            <UML:Generalization.general>
              <UML:Class href="http://www.omg.org/spec/UML/20090901#Class"/>
            </UML:Generalization.general>
          </UML:Generalization>
        </UML:Stereotype>

        <UML:Stereotype name="abstract" visibility="public">
          <UML:Generalization>
            <UML:Generalization.general>
              <UML:Class href="http://www.omg.org/spec/UML/20090901#Class"/>
            </UML:Generalization.general>
          </UML:Generalization>
          <UML:Generalization>
            <UML:Generalization.general>
              <UML:Association href="http://www.omg.org/spec/UML/20090901#Association"/>
            </UML:Generalization.general>
          </UML:Generalization>
          <UML:Generalization>
            <UML:Generalization.general>
              <UML:AssociationClass href="http://www.omg.org/spec/UML/20090901#AssociationClass"/>
            </UML:Generalization.general>
          </UML:Generalization>
        </UML:Stereotype>

        <!-- Definierte Datentypen -->
        <UML:DataType name="Double"/>
        <UML:DataType name="Long"/>
        <UML:DataType name="Integer"/>
        <UML:DataType name="Boolean"/>
        <UML:DataType name="String"/>

        <!-- Tag-Definition "redefines" -->
        <UML:TagDefinition name="redefines" visibility="public">
          <UML:Multiplicity>
            <UML:Multiplicity.range>
              <UML:MultiplicityRange lower="0" upper="1"/>
            </UML:Multiplicity.range>
          </UML:Multiplicity>
        </UML:TagDefinition>

      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>
