<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ArgoUML</XMI.exporter>
      <XMI.exporterVersion>0.34</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="model_untitledModel" name="untitledModel">
      <UML:Namespace.ownedElement>
        
        <!-- Definition der StateMachine -->
        <UML:StateMachine xmi.id="stateMachine_1" name="StateMachine">
          <UML:StateMachine.top>
            <UML:CompositeState xmi.id="compositeState_top" name="top" isRegion="false" isConcurrent="false" isComposite="true">
              
              <!-- Definition des Pseudostate -->
              <UML:CompositeState.subvertex>
                <UML:Pseudostate xmi.id="pseudostate_1" name="junction" kind="junction"/>
                
                <!-- Definition des SimpleState -->
                <UML:SimpleState xmi.id="simpleState_1" name="SimpleState"/>
              </UML:CompositeState.subvertex>

              <!-- Definition der Transition -->
              <UML:Transition xmi.id="transition_1" name="transition" source="pseudostate_1" target="simpleState_1">
                <UML:Transition.guard>
                  <UML:Guard xmi.id="guard_1" name="guard"/>
                </UML:Transition.guard>
              </UML:Transition>

            </UML:CompositeState>
          </UML:StateMachine.top>
        </UML:StateMachine>

      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>