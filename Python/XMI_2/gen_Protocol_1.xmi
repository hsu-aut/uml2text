<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ArgoUML</XMI.exporter>
      <XMI.exporterVersion>0.34</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="m1" name="Модель UML">
      <UML:Namespace.ownedElement>
        <!-- Definition der Datentypen -->
        <UML:Package xmi.id="Datatypes" name="Datatypes">
          <UML:Namespace.ownedElement>
            <UML:DataType xmi.id="int" name="int"/>
            <UML:DataType xmi.id="char" name="char"/>
            <UML:DataType xmi.id="bool" name="bool"/>
            <UML:DataType xmi.id="float" name="float"/>
            <UML:DataType xmi.id="double" name="double"/>
            <UML:DataType xmi.id="short" name="short"/>
            <UML:DataType xmi.id="long" name="long"/>
            <UML:DataType xmi.id="unsigned_int" name="unsigned int"/>
            <UML:DataType xmi.id="unsigned_short" name="unsigned short"/>
            <UML:DataType xmi.id="unsigned_long" name="unsigned long"/>
            <UML:DataType xmi.id="string" name="string"/>
          </UML:Namespace.ownedElement>
        </UML:Package>

        <!-- Definition der Hauptklassen im Logical View -->
        <UML:Package xmi.id="Logical_View" name="Logical View">
          <UML:Namespace.ownedElement>
            <!-- User Klasse -->
            <UML:Class xmi.id="class_User" name="User" visibility="public" isAbstract="false"/>

            <!-- Client Klasse -->
            <UML:Class xmi.id="class_Client" name="Client" visibility="public" isAbstract="false"/>
            
            <!-- Server Klasse -->
            <UML:Class xmi.id="class_Server" name="Server" visibility="public" isAbstract="false"/>

            <!-- KeyGenerator Klasse -->
            <UML:Class xmi.id="class_KeyGenerator" name="KeyGenerator" visibility="public" isAbstract="false"/>
            
            <!-- ObjectStorage Klasse -->
            <UML:Class xmi.id="class_ObjectStorage" name="ObjectStorage" visibility="public" isAbstract="false"/>
            
            <!-- Db Klasse -->
            <UML:Class xmi.id="class_Db" name="Db" visibility="public" isAbstract="false"/>
            
            <!-- GPS Klasse -->
            <UML:Class xmi.id="class_GPS" name="GPS" visibility="public" isAbstract="false"/>
            
            <!-- SearchEngine Klasse -->
            <UML:Class xmi.id="class_SearchEngine" name="SearchEngine" visibility="public" isAbstract="false"/>
          </UML:Namespace.ownedElement>
        </UML:Package>

        <!-- Interaktionen in Form von Sequenzdiagrammen -->

        <!-- Diagram "add content" -->
        <UML:Collaboration xmi.id="collab_addContent" name="add_content">
          <UML:Collaboration.interaction>
            <UML:Interaction xmi.id="inter_addContent" name="add_content_interaction"/>
          </UML:Collaboration.interaction>
        </UML:Collaboration>

        <!-- Diagram "search" -->
        <UML:Collaboration xmi.id="collab_search" name="search">
          <UML:Collaboration.interaction>
            <UML:Interaction xmi.id="inter_search" name="search_interaction"/>
          </UML:Collaboration.interaction>
        </UML:Collaboration>

        <!-- Diagram "subscribe to channel" -->
        <UML:Collaboration xmi.id="collab_subscribe" name="subscribe_to_channel">
          <UML:Collaboration.interaction>
            <UML:Interaction xmi.id="inter_subscribe" name="subscribe_interaction"/>
          </UML:Collaboration.interaction>
        </UML:Collaboration>

      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>