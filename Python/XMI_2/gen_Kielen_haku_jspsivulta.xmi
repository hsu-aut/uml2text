<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ArgoUML</XMI.exporter>
      <XMI.exporterVersion>0.34</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="model_LanguageManagement" name="LanguageManagement">
      <UML:Namespace.ownedElement>

        <!-- Definition der Datentypen -->
        <UML:DataType xmi.id="datatype_int" name="int"/>
        <UML:DataType xmi.id="datatype_char" name="char"/>
        <UML:DataType xmi.id="datatype_bool" name="bool"/>
        <UML:DataType xmi.id="datatype_float" name="float"/>
        <UML:DataType xmi.id="datatype_double" name="double"/>
        <UML:DataType xmi.id="datatype_long" name="long"/>
        <UML:DataType xmi.id="datatype_short" name="short"/>
        <UML:DataType xmi.id="datatype_string" name="string"/>

        <!-- Definition der Klasse LanguageManager -->
        <UML:Class xmi.id="class_LanguageManager" name="LanguageManager" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Operation xmi.id="operation_getTextResource" name="getTextResource" visibility="public" isAbstract="false" isQuery="false">
              <UML:Parameter xmi.id="param_language" name="language" type="datatype_string" direction="in"/>
              <UML:Parameter xmi.id="param_pageName" name="pageName" type="datatype_string" direction="in"/>
            </UML:Operation>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <!-- Definition der Klasse ResourceBundle -->
        <UML:Class xmi.id="class_ResourceBundle" name="ResourceBundle" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Operation xmi.id="operation_getString" name="getString" visibility="public" isAbstract="false" isQuery="false">
              <UML:Parameter xmi.id="param_key" name="key" type="datatype_string" direction="in"/>
            </UML:Operation>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <!-- Definition der Klasse HttpSession -->
        <UML:Class xmi.id="class_HttpSession" name="HttpSession" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Operation xmi.id="operation_getAttribute" name="getAttribute" visibility="public" isAbstract="false" isQuery="false">
              <UML:Parameter xmi.id="param_name" name="name" type="datatype_string" direction="in"/>
            </UML:Operation>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <!-- Definition der Klasse JSP page -->
        <UML:Class xmi.id="class_JSPPage" name="JSPPage" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <!-- Operationen und deren Parameter kÃ¶nnten hier weiter spezifiziert werden -->
          </UML:Namespace.ownedElement>
        </UML:Class>

      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>